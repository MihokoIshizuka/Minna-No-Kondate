<table class="table">
  <thead>
    <tr>
      <th></th>
      <th>名前</th>
      <th>紹介</th>
      <th>よく使うタグ</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% members.each do |member| %>
    <tr>
      <td>
        <%= link_to member_path(member) do %>
          <%= image_tag member.get_profile_image, size:"65x65" %>
        <% end %>
      </td>
      <td>
        <%= link_to member_path(member) do %>
          <%= member.name %>
        <% end %>
      </td>
      <td><%= member.introduction %></td>
      
    </tr>
    <% end %>
  </tbody>
</table>