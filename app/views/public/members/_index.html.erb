<table class="table">
  <thead>
    <tr>
      <th></th>
      <th>name</th>
      <th>introduction</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% members.each do |member| %>
    <tr>
      <td>
        <%= link_to member_path(member) do %>
          <%= image_tag member.get_profile_image, size:"65x65" %>
        <% end %>
      </td>
      <td>
        <%= link_to member_path(member) do %>
          <%= member.name %>
        <% end %>
      </td>
      <td><%= member.introduction %></td>
      <!--フォロー機能非同期-->
      <td id="relationships_button">
        <%= render 'public/relationships/relationships', member: member %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>